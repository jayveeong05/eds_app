{
  "functions": {
    "backend/api/*.php": {
      "runtime": "vercel-php@0.7.0"
    },
    "backend/api/admin/*.php": {
      "runtime": "vercel-php@0.7.0"
    },
    "backend/admin/*.php": {
      "runtime": "vercel-php@0.7.0"
    },
    "backend/index.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/backend/admin/$1"
    },
    {
      "src": "/admin",
      "dest": "/backend/admin/index.php"
    },
    {
      "src": "/",
      "dest": "/backend/index.php"
    }
  ]
}